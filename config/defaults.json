{
  "provider": "google",
  "runtime": "pi",
  "userName": "",
  "knowledgeCutoff": "End of May 2025",
  "includeRawChunks": true,
  "observabilityEnabled": true,
  "observability": {
    "provider": "langfuse",
    "baseUrl": "https://cloud.langfuse.com",
    "otelEndpoint": "https://cloud.langfuse.com/api/public/otel/v1/traces"
  },
  "providerOptions": {
    "google": {
      "thinkingConfig": {
        "includeThoughts": true,
        "thinkingLevel": "high"
      }
    },
    "openai": {
      "reasoningEffort": "high",
      "reasoningSummary": "detailed",
      "textVerbosity": "high"
    },
    "anthropic": {
      "thinking": {
        "type": "enabled",
        "budgetTokens": 32000
      }
    },
    "codex-cli": {
      "reasoningEffort": "high",
      "reasoningSummary": "detailed",
      "textVerbosity": "high"
    }
  },
  "harness": {
    "reportOnly": true,
    "strictMode": false
  }
}
